{
  "name": "calf",
  "version": "1.0.0",
  "description": "The original monolithic codebase for AdRhino",
  "main": "index.js",
  "scripts": {
    "start": "concurrently 'mongod' 'npm run server' 'npm run lint'",
    "server": "NODE_PATH=$NODE_PATH:. node -r dotenv/config -r babel-core/register index.js",
    "lint": "nodemon --exec 'standard | notify'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrhino/calf.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adrhino/calf/issues"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "homepage": "https://github.com/adrhino/calf#readme",
  "babel": {
    "presets": [
      "es2015",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "dependencies": {
    "auth0-lock": "^9.0.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.5.1",
    "babel-eslint": "^5.0.0",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "babelify": "^7.2.0",
    "brfs": "^1.4.3",
    "browserify": "^13.0.0",
    "debug": "^2.2.0",
    "graphiql": "^0.5.0",
    "graphql": "^0.4.18",
    "joi": "^8.0.3",
    "koa": "^2.0.0-alpha.3",
    "koa-bodyparser": "^3.0.0",
    "koa-browserify-middleware": "^1.0.0",
    "koa-connect": "https://github.com/lourd/koa-connect.git",
    "koa-convert": "^1.2.0",
    "koa-generic-session": "^1.6",
    "koa-graphql": "^0.4.4",
    "koa-jwt": "^1.1.2",
    "koa-mount": "git://github.com/coderhaoxin/koa-mount.git#next-by-promise",
    "koa-passport": "^2.0.1",
    "koa-route": "^3.0.0",
    "koa-static": "^2.0.0",
    "passport": "^0.3.2",
    "passport-auth0": "^0.5.1",
    "promised-mongo": "^1.2.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-functional": "^1.2.0"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "dotenv": "^2.0.0",
    "cache-require-paths": "^0.2.0",
    "envify": "^3.4.0",
    "foreman": "^1.4.1",
    "node-notifier": "^4.5.0",
    "nodemon": "^1.9.1",
    "rewire": "^2.5.1",
    "standard": "^5.4.1",
    "standard-format": "^2.1.1"
  }
}
